FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include          "include/freestreamConditions"

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform $omega;

boundaryField
{
    frontAndBack_right
    {
        type            wedge;
    }
    "(hub|shroud)"
    {
        type            omegaWallFunction;
        value           uniform $omega;    
	}
    frontAndBack_left
    {
        type            wedge;
    }
    outlet
    {
        type            turbulentMixingLengthFrequencyInlet;
        mixingLength    0.09;
        value           uniform $omega;
    }
    inlet
    {
        type            turbulentMixingLengthFrequencyInlet;
        mixingLength    0.09;
        value           uniform $omega;
    }
}


// ************************************************************************* //
