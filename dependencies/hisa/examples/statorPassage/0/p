FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include          "include/freestreamConditions"

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform $p;

boundaryField
{
    frontAndBack_right
    {
        type            wedge;
    }
    "(hub|shroud)"
    {
        type            characteristicWallPressure;
	#include          "include/freestreamConditions"
        value           uniform $p;
    }
    frontAndBack_left
    {
        type            wedge;
    }
    outlet
    {
        type            fixedValue;
	#include          "include/freestreamConditions"
        value           uniform $p;

    }
    inlet
    {
        type            totalPressure;
        p0              uniform 105325;
        value           uniform 101325;
    }
}


