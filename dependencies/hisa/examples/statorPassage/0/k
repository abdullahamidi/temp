FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include          "include/freestreamConditions"

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $k;

boundaryField
{
    frontAndBack_right
    {
        type            wedge;
    }
    "(hub|shroud)"
    {
        type            kLowReWallFunction;
        value           uniform 1e-11;
    }
    frontAndBack_left
    {
        type            wedge;
    }
    outlet
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.01;
        value           uniform $k;
    }
    inlet
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.01;
        value           uniform $k;
    }
}


// ************************************************************************* //
