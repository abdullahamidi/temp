FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    location        "system";
    object          controlDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application       cea;

startFrom         startTime;

startTime         0;

stopAt            endTime;

endTime           50;

deltaT            1;                // Implicit

writeControl      timeStep;

writeInterval     1; 

purgeWrite        0;

writeFormat       ascii;

writePrecision    20;

writeCompression  uncompressed;

timeFormat        general;

timePrecision     6;

graphFormat       raw;

runTimeModifiable yes;

adjustTimeStep    yes;

maxCo             1e20;               // Steady

maxDeltaT         1;

// ************************************************************************* //
