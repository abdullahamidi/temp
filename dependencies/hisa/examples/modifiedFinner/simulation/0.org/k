FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 104;  // = 3/2*(I*|U|)^2 for I = 0.02 & U = 412

boundaryField
{
    "(body|fin1|fin2|fin3|fin4)"
    {
        type            kLowReWallFunction;
        value           uniform 1e-11;
    }

    "(farfield)"
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.02; // 2% turbulence intensity
        value           $internalField;
    }

    "(symm)"
    {
        type            symmetry;
        value           uniform 0;
    }


}


// ************************************************************************* //
