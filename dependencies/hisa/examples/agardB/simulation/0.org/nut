FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    "(body|bodyBase)"
    {
        type            boundaryCorrectedNutLowReWallFunction;              // y+ < 1
        value           uniform 0;
    }
    
    "(farfield)"
    {
        type            calculated;              
        value           uniform 0;
    }
    
    symmetry
    {
        type            symmetry;
        value           uniform 0;
    }
    

}


